<Window x:Class="SkypeMenuAdmin.LoginUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkypeMenuAdmin"
        mc:Ignorable="d"
         ResizeMode="NoResize"
         WindowStartupLocation="CenterScreen" 
        Title="Login User" Height="156" Width="270">
    <Grid>
        <Label x:Name="lblUser" Content="Username" HorizontalAlignment="Left" Margin="17,26,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Height="23" Margin="85,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="145"/>
        <Label x:Name="lblPass" Content="Password" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="17,50,0,0" Width="63"/>
        <Button x:Name="btnlogin" Content="Login" HorizontalAlignment="Left" Margin="85,82,0,0" VerticalAlignment="Top" Width="75" Click="btnlogin_Click"/>
        <PasswordBox x:Name="passwordBox1" HorizontalAlignment="Left" Margin="85,54,0,0" PasswordChar="*" VerticalAlignment="Top" Width="145" Height="22"/>

    </Grid>
</Window>
